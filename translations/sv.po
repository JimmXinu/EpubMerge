msgstr "Påbörja sammanslagning..."

#: dialogs.py:174
msgid "Move selected books up the list"
msgstr "Flytta valda böcker uppåt i listan"

#: dialogs.py:179
msgid "Remove selected books from the list"
msgstr "Ta bort valda böcker från listan"

#: dialogs.py:184
msgid "Move selected books down the list"
msgstr "Flytta valda böcker neråt i listan"

#: dialogs.py:223
msgid "Author(s)"
msgstr "Författare"

#: dialogs.py:223
msgid "Series"
msgstr "Serier"

#: dialogs.py:223
msgid "Title"
msgstr "Titel"

#: dialogs.py:272
msgid "Are you sure you want to remove this book from the list?"
msgstr "Är du säker du vill ta bort denna bok från listan?"

#: dialogs.py:274
msgid "Are you sure you want to remove the selected %d books from the list?"
msgstr "Är du säker du vill ta bort valda %d böcker från listan?"

